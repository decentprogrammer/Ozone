@page
@model IndexModel
@{
    Layout = "_WflowLayout";
}

<input type="hidden" id="userUnitId" value="@Model.userUnitId" />
<div class="home-section dashboard">
    <div data-collapse="medium" data-animation="default" data-duration="400" role="banner" class="navbar w-nav">
        <div class="nav-container w-container">
            <a href="dashboard.html" class="logo w-nav-brand"><img src="~/wf/images/Ozone-Logo-2.svg" width="90" alt="" class="image-4"></a>
            <nav role="navigation" class="w-nav-menu">
                <a href="#" class="nav-link login-name w-nav-link">Naseralhubail@gmail.com</a>
                <div class="profile-pic"><img src="~/wf/images/Profile.svg" width="40" alt="" class="image-4"></div>
            </nav>
            <div class="w-nav-button">
                <div class="w-icon-nav-menu"></div>
            </div>
        </div>
    </div>
    <div class="manage-page-container w-container">
        <div class="manage-home-screen">
            <div class="manage-nav-bar">
                <div data-collapse="medium" data-animation="default" data-duration="400" role="banner" class="navbar-2 w-nav">
                    <div class="container-2 w-container">
                        <a href="#" class="brand w-nav-brand">
                            <div class="manage-page-title">Manage<br>‍</div>
                        </a>
                        <nav role="navigation" class="nav-menu w-nav-menu">
                            <a id="createChecklist" class="manage-nav-link w-nav-link">Checklist</a>
                            <a href="#" class="manage-nav-link w-nav-link">About</a>
                            <a href="#" class="manage-nav-link w-nav-link">Contact</a>
                        </nav>
                        <div class="w-nav-button">
                            <div class="w-icon-nav-menu"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="manage-page-workspace-containter-2">
                <div style="display:none" id="createChecklistPanelWraper" class="panels-wraper-2">
                    @await Html.PartialAsync("./ManagePartials/_Category.cshtml", Model.categoryModel)
                    @await Html.PartialAsync("./ManagePartials/_Element.cshtml", Model.elementModel)
                </div>
            </div>
        </div>
    </div>
    @*<div id="test" style="color:aliceblue">
            <p></p>
        </div>*@
</div>
<div style="opacity:0" class="ozone-modal">
    <div data-w-id="1075fbc2-4c90-ef1d-fdb1-ba98d9e993ab" class="modal-close-bg"></div>
    @await Html.PartialAsync("./ManagePartials/_ChecklistModal.cshtml")
</div>




@section Scripts {
    <script src="~/js/OzoneGlobal.js" type="text/javascript"></script>
    <script src="~/js/checklist.js" type="text/javascript"></script>

}